{
  "name": "flex-app",
  "version": "0.1.0",
  "private": true,
  "main": "lib/index.js",
  "engines": {
    "node": "16"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint **/*.ts **/*.tsx",
    "lint:fix": "npm run lint -- --fix",
    "emu": "firebase emulators:start",
    "test": "jest",
    "tsc:functions": "tsc -p functions",
    "serve": "npm run build && npm run \"tsc:functions\" && firebase emulators:start",
    "shell": "npm run build && firebase functions:shell",
    "deploy": "firebase deploy --only functions,hosting",
    "deploy:functions": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/system": "^4.12.1",
    "@react-google-maps/api": "^2.5.0",
    "@types/react-calendar": "^3.4.3",
    "axios": "^0.24.0",
    "firebase": "^9.2.0",
    "firebase-admin": "^10.0.0",
    "firebase-functions": "^3.16.0",
    "interactjs": "^1.10.11",
    "material-table": "^1.69.3",
    "moment": "^2.29.1",
    "next": "11.1.2",
    "react": "17.0.2",
    "react-calendar": "^3.5.0",
    "react-dom": "17.0.2",
    "react-social-login-buttons": "^3.6.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.106",
    "@swc/jest": "^0.2.5",
    "@types/jest": "^27.0.2",
    "@types/react": "17.0.26",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "babel-jest": "^26.6.1",
    "eslint": "7.32.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "11.1.2",
    "eslint-plugin-import": "^2.25.2",
    "firebase-tools": "^9.22.0",
    "jest": "^27.3.1",
    "typescript": "4.4.3"
  }
}
